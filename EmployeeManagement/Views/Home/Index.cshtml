@model IEnumerable<Employee>

@{
   
    ViewBag.Title = "Employee List";
}
<table style="width:100%;">

    <thead>
        <tr>
            <td>ID</td>
            <td>Name</td>
            <td>Department</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>
                    @employee.Id
            </td>
            <td>@employee.Name</td>
            <td>@employee.Department</td>
        </tr>
    }
    </tbody>
</table>
